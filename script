pipeline {
    agent any
    stages{
        stage("Cleaning stage"){
            steps{
                echo "In Stage Clean"
                bat "mvn clean"
            }
        }
        stage("Testing stage"){
            steps{
                echo "In Stage Test"
                bat "mvn test"
            }
        }
        stage("Packaging stage"){
            steps{
                echo "In Stage Package"
                bat "mvn package"
            }
        }
    }
}
